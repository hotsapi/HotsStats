<stats:HeroesWindow x:Class="StatsDisplay.Stats.RecapStatsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
		xmlns:fetcher="clr-namespace:StatsFetcher;assembly=StatsFetcher"
        xmlns:local="clr-namespace:StatsDisplay"
        xmlns:stats="clr-namespace:StatsDisplay.Stats"
        mc:Ignorable="d"
        DataContext="{Binding RelativeSource={RelativeSource Self}}"
		d:DataContext="{Binding Source={StaticResource DesignViewModel}}"
		Top="{Binding Settings.RecapStatsWindowTop, Mode=TwoWay}" Left="{Binding Settings.RecapStatsWindowLeft, Mode=TwoWay}">
	<Window.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceDictionary Source="/Styles.xaml" />
			</ResourceDictionary.MergedDictionaries>
			<stats:MockViewModel x:Key="DesignViewModel"/>
		</ResourceDictionary>
	</Window.Resources>
    <Grid>
        
    </Grid>
</stats:HeroesWindow>

